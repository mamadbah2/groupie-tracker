{{ template "base" .}}

{{ define "content" }}

{{ if eq .AllArtist nil }}
    <h1>No value input</h1>
    <a href="/"> << Go back </a>
{{end}}
<div class="container">
    {{ range .AllArtist }}
    <a href="detail?Id={{ .Id }}">
        <div class="box">
            <div class="image">
                <img src="{{ .Image }}" alt="#">
            </div>
            <h3>
                {{ .Name }}
            </h3>
        </div>
    </a>
    {{ end }}
</div>

{{ end }}